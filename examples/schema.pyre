
// User table
record User {
    id:   Int
    name: String
    status: Status
}

// My status type
type Status
    = Active
    | Inactive
    | Special { reason: String }
    | Inactive
    | Special2 {
        reason2: String
        error: String
      }


record Account {
    id:   Int
    name: String
    status: Status
}
