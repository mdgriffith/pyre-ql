{
  "name": "@pyre/server",
  "version": "0.1.0",
  "description": "Server runtime for executing generated Pyre queries",
  "license": "MIT",
  "type": "module",
  "main": "./index.ts",
  "module": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./index.ts"
    },
    "./query": {
      "types": "./query.ts",
      "default": "./query.ts"
    },
    "./wasm": {
      "types": "./wasm.ts",
      "default": "./wasm.ts"
    },
    "./schema": {
      "types": "./schema.ts",
      "default": "./schema.ts"
    },
    "./sync": {
      "types": "./sync-api.ts",
      "default": "./sync-api.ts"
    },
    "./query-sync": {
      "types": "./query-sync.ts",
      "default": "./query-sync.ts"
    },
    "./runtime/sql": {
      "types": "./runtime/sql.ts",
      "default": "./runtime/sql.ts"
    },
    "./runtime/runner": {
      "types": "./runtime/runner.ts",
      "default": "./runtime/runner.ts"
    }
  },
  "dependencies": {
    "@pyre/core": "0.1.0",
    "@libsql/client": "^0.14.0",
    "zod": "^4.1.12"
  },
  "files": [
    "index.ts",
    "init.ts",
    "query-sync.ts",
    "query.ts",
    "runtime",
    "schema.ts",
    "sync-api.ts",
    "sync.ts",
    "wasm.ts",
    "wasm",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  }
}
