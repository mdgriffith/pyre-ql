{
  "name": "pyre-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "bun run --filter '*' build",
    "typecheck": "bun run --filter '*' typecheck",
    "release:check": "bun run typecheck",
    "release:pack": "bun run ./scripts/release-pack.mjs",
    "release:smoke": "bun run ./scripts/release-smoke.mjs",
    "release:clean": "bun run ./scripts/release-clean.mjs"
  }
}
